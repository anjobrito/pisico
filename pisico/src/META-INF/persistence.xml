<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="pisicoPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>prj.org.pisico.model.TbConta</class>
    <class>prj.org.pisico.model.TbAgendaHasTbPaciente</class>
    <class>prj.org.pisico.model.TbPerfil</class>
    <class>prj.org.pisico.model.TbPerfilHasTbUsuario</class>
    <class>prj.org.pisico.model.TbUf</class>
    <class>prj.org.pisico.model.TbUsuario</class>
    <class>prj.org.pisico.model.TbPaciente</class>
    <class>prj.org.pisico.model.TbConvenio</class>
    <class>prj.org.pisico.model.TbStatus</class>
    <class>prj.org.pisico.model.TbCidade</class>
    <class>prj.org.pisico.model.TbBanco</class>
    <class>prj.org.pisico.model.TbPerfil_1</class>
    <class>prj.org.pisico.model.TbAgenda</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pisico?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
